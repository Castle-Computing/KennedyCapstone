<html>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<div class = "row">
    <div class = "col">
      <script>
      let url = '/dynamicjson?rekl=#(rekl)&islandora=#(islandora)&cpsca=#(cpsca)';

      fetch(url)
